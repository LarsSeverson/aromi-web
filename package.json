    "graphql": "^16.10.0",
    "lodash": "^4.17.21",
    "neverthrow": "^8.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-loading-skeleton": "^3.5.0",
    "react-svg": "^16.3.0",
    "react-textarea-autosize": "^8.5.9",
