{
  "name": "aromi-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"yarn run dev-css\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "dev-css": "npx tailwindcss -i \"$PWD/src/styles/input.css\" -o \"$PWD/src/styles/output.css\" --watch",
    "codegen": "graphql-codegen --config src/scripts/codegen.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.13.4",
    "@headlessui/react": "^2.2.0",
    "@tanstack/react-router": "1.112.12",
    "@tanstack/react-virtual": "^3.13.4",
    "aws-amplify": "6.13.2",
    "clsx": "^2.1.1",
    "graphql": "^16.10.0",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-svg": "^16.3.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript": "^4.1.5",
    "@tanstack/eslint-plugin-router": "^1.114.29",
    "@tanstack/react-router-devtools": "^1.114.27",
    "@tanstack/router-plugin": "^1.114.27",
    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "eslint": "8.57.1",
    "eslint-config-standard-react": "^13.0.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "standard": "^17.1.2",
    "tailwindcss": "3.4.17",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
